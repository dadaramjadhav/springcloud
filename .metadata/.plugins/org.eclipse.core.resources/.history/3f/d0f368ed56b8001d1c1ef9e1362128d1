#server.port=7092
#spring.application.name=consumer
#eureka.client.serviceUrl.defaultZone=http://eureka-server:7090/eureka

server:
   port: 7092
eureka:
  instance:
    hostname: consumer
    prefer-ip-address: true
  client:
    registerWithEureka: true
    fetchRegistry: true
    serviceUrl:
            defaultZone: http://eureka-server:7090/eureka/